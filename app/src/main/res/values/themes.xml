<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.GPS" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:windowBackground">@color/white</item>
        <item name="android:statusBarColor">@color/white</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>

        <item name="textAllCaps">false</item>
        <item name="android:textViewStyle">@style/MyTheme</item>
        <item name="android:tint">@color/black</item>


        <!-- Customize your theme here. -->
        <item name="android:windowSplashScreenBackground" tools:targetApi="s">@color/black</item>
    </style>



    <style name="BottomNavication">
        <item name="android:background">@color/white</item>
    </style>

    <style name="BackgroundTintImg">
        <item name="android:backgroundTint">@color/black</item>
    </style>

    <style name="Text">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="BackgroundTint">
        <item name="android:backgroundTint">#FFFFFF</item>
    </style>

    <style name="BackgroundTint1">
        <item name="android:backgroundTint">#FFFFFF</item>
    </style>

    <style name="Button">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:iconTint" tools:targetApi="o">@color/black</item>

    </style>

    <style name="MyTheme" parent="@android:style/TextAppearance">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="Theme.GPS" parent="Base.Theme.GPS" />


</resources>