<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.GPS" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:windowBackground">@color/white</item>
        <item name="android:statusBarColor">@color/white</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>

        <item name="textAllCaps">false</item>
        <item name="android:textViewStyle">@style/MyTheme</item>


        <!-- Customize your theme here. -->
    </style>

    <style name="BackgroundItem">
        <item name="android:background">#C4C4C4</item>
    </style>

    <style name="BottomNavication">
        <item name="android:background">@color/white</item>
    </style>

    <style name="BackgroundTintImg">
        <item name="android:backgroundTint">@color/black</item>
    </style>

    <style name="Text">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="BackgroundTint">
        <item name="android:backgroundTint">#FFFFFF</item>
    </style>

    <style name="BackgroundDialog">
        <item name="android:backgroundTint">#FFFFFF</item>
    </style>

    <style name="TextDialog">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="BackgroundTint1">
        <item name="android:backgroundTint">#FFFFFF</item>
    </style>

    <style name="Button">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:iconTint" tools:targetApi="o">@color/black</item>

    </style>

    <style name="BUTTONSETTINGOPSTION">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">11dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:backgroundTint">@color/backgroundbuttonsettingopstions</item>
        <item name="cornerRadius">25dp</item>
        <item name="strokeColor">@color/white</item>
        <item name="android:textColor">#999999</item>
    </style>

    <style name="ButtonTipNight">
        <item name="android:textColor">@color/black</item>
        <item name="android:backgroundTint">@color/white</item>
    </style>

    <style name="ButtonTipDrak">
        <item name="android:textColor">@color/white</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="MyTheme" parent="@android:style/TextAppearance">
        <item name="android:textColor">#000000</item>
        <item name="android:textAllCaps">false</item>

    </style>

    <style name="BckGroundTip">
        <item name="android:background">@color/white</item>
    </style>

    <style name="Theme.GPS" parent="Base.Theme.GPS" />


</resources>